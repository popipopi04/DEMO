[
    {
      "name": "prodcontainer",
      "image": "nginx:latest",
      "cpu": ${fargate_cpu},
      "memory": ${fargate_memory},
      "networkMode": "awsvpc",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mpulsebackend-prod-green",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
    },



    "portMappings": [
        {
          "containerPort": ${app_port},
          "hostPort": ${app_port}
        }
      ]
    }
  ]
